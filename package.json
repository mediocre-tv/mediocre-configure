{
  "name": "mediocre-configurer",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "eslint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "prettier": "npx prettier -c '{**/*,*}.{js,ts,jsx,tsx,css}'",
    "lint": "conc -c auto npm:prettier npm:eslint",
    "eslint-fix": "eslint . --ext ts,tsx --fix",
    "prettier-fix": "npx prettier -w '{**/*,*}.{js,ts,jsx,tsx,css}'",
    "lint-fix": "npm run eslint-fix && npm run prettier-fix",
    "preview": "vite preview"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.55",
    "@types/react-dom": "^18.2.19",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@vitejs/plugin-react": "^4.2.1",
    "concurrently": "^8.2.2",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "prettier": "^3.2.5",
    "typescript": "^5.2.2",
    "vite": "^5.1.0"
  }
}
